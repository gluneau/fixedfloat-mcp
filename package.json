{
  "name": "fixedfloat-mcp",
  "publishConfig": {
    "access": "public"
  },
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "start": "node dist/index.js",
    "inspector": "npx @modelcontextprotocol/inspector dist/index.js",
    "build": "rimraf dist && tsc && shx chmod +x dist/index.js",
    "prepare": "npm run build",
    "watch": "tsc --watch"
  },
  "author": "gregory.luneau@gmail.com",
  "license": "ISC",
  "description": "An MCP server that enables AI assistants to interact with the FixedFloat cryptocurrency exchange",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.6.1",
    "crypto": "^1.0.1",
    "node-fetch": "^2.6.9",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^22.13.8",
    "@types/node-fetch": "^2.6.9",
    "rimraf": "^5.0.5",
    "shx": "^0.3.4",
    "typescript": "^5.8.2"
  },
  "keywords": [
    "fixedfloat",
    "cryptocurrency",
    "exchange",
    "mcp",
    "model-context-protocol",
    "ai",
    "assistant",
    "crypto",
    "web3"
  ],
  "bin": {
    "fixedfloat-mcp": "dist/index.js"
  }
}